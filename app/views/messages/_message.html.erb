<% if message.user.id == current_user.id%>
      <small>
        <i><%= message.created_at.strftime("%b %e at %l:%M %p") %></i>
      </small>
      <p><%= message.content %></p>
<% else %>
      <small>
        <i><%= message.created_at.strftime("%b %e at %l:%M %p") %></i>
      </small>
      <p><%= message.content %></p>
<% end %>
